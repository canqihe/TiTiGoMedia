<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.trueu.titigomedia.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_product"
        android:name="com.trueu.titigomedia.ui.product.ProductFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_product" />

    <fragment
        android:id="@+id/navigation_analytics"
        android:name="com.trueu.titigomedia.ui.analytics.AnalyticsFragment"
        android:label="@string/title_analytics"
        tools:layout="@layout/fragment_product" />

    <fragment
        android:id="@+id/navigation_user"
        android:name="com.trueu.titigomedia.ui.user.UserFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_user" />
</navigation>